class MyArray {
	// Data Structure
	int[] array;
	void createArr (int[] arr) {
		array = new int[arr.length];
		for (int i = 0; i < arr.length; i++) {
			array[i] = arr[i];
		}
	}
	
	int sumArr() {
		int sum=0;
		for (int i=0; i<array.length; i++) {
			sum+=array[i];
		}
		return sum;
	}
}


public class ArrayTest {
	
	public static void main(String[] args) {
		int[] score = {90, 85, 100, 65, 70};
		MyArray arr1 = new MyArray(); //object creation
		arr1.createArr(score);
	}
}
